{
    "cache": [
        {
            "name": "System Log Maintenance",
            "category": "Routine",
            "zone": "/var/log",
            "commands": [
                "sudo logrotate -f /etc/logrotate.conf",
                "sudo systemctl reload rsyslog",
                "df -h /var/log",
                "find /var/log -name '*.gz' -mtime +7 -delete"
            ]
        },
        {
            "name": "Nginx Log Rotation",
            "category": "Routine",
            "zone": "/var/log/nginx",
            "commands": [
                "sudo nginx -t",
                "sudo systemctl reload nginx",
                "tail -20 /var/log/nginx/access.log",
                "tail -20 /var/log/nginx/error.log"
            ]
        },
        {
            "name": "Backup Operations",
            "category": "Routine",
            "zone": "/mnt/backup",
            "commands": [
                "tar -czf /mnt/backup/config-$(date +%Y%m%d).tar.gz /etc/nginx /etc/ssh",
                "rsync -av /var/log/ /mnt/backup/logs/",
                "find /mnt/backup -name '*.tar.gz' -mtime +30 -delete"
            ]
        }
    ],
    "honeytoken": [
        "echo 'AWS_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE' > .aws/credentials",
        "echo 'DB_PASSWORD=admin' > config.php"
    ],
    "vuln": [
        "chmod 777 -R /var/www/html/uploads",
        "echo 'PermitRootLogin yes' >> /etc/ssh/sshd_config"
    ],
    "narrative": [
        {
            "name": "Narrative Progress (Log)",
            "category": "Narrative",
            "zone": "{home_dir}/repos/backend-api",
            "commands": [
                "echo 'Progress on task: {task}' >> work_log.txt",
                "git add work_log.txt",
                "git commit -m 'Update: {task}'"
            ]
        },
        {
            "name": "Narrative Progress (Code)",
            "category": "Narrative",
            "zone": "{home_dir}/repos/backend-api/src",
            "commands": [
                "echo '# TODO: Finish {task}' >> todo.py",
                "git add todo.py",
                "git commit -m 'WIP: {task}'"
            ]
        }
    ],
    "fuzzing": {
        "files": [
            "utils.py",
            "config.py",
            "app.py",
            "server.go",
            "routes.js"
        ],
        "commit_messages": [
            "Fix typo",
            "WIP",
            "Update logic",
            "Refactor",
            "Cleanup",
            "Hotfix"
        ]
    },
    "typos": {
        "git": [
            "gti",
            "got",
            "fit"
        ],
        "grep": [
            "gerp",
            "grpe"
        ],
        "echo": [
            "ehco",
            "ecoh"
        ],
        "ls": [
            "sl",
            "l"
        ],
        "docker": [
            "doker",
            "docekr"
        ]
    },
    "fallback_maintenance": {
        "name": "System Health Check",
        "category": "Routine",
        "zone": "{home_dir}",
        "commands": [
            "uptime",
            "df -h",
            "free -h",
            "ps aux | head -20"
        ]
    },
    "triggered_response": {
        "name": "Triggered Response: {keyword}",
        "category": "Responsive",
        "zone": "{home_dir}",
        "commands": [
            "echo 'Detected trigger: {keyword}' >> /var/log/triggers.log",
            "date >> /var/log/triggers.log"
        ]
    }
}