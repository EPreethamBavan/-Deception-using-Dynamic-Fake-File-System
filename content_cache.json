{
    "forecast_queue": [
        {
            "user": "svc_ci",
            "name": "CI build and test execution",
            "category": "Routine",
            "zone": "/builds/awesome-app",
            "commands": [
                "npm install",
                "npm run build",
                "npm test"
            ]
        },
        {
            "user": "svc_ci",
            "name": "CI build failure detected",
            "category": "Anomaly",
            "zone": "/builds/awesome-app",
            "commands": [
                "echo 'Build failed: Unit tests not passing.' > build_log.txt",
                "send_notification --user sys_bob --message \"CI build for awesome-app failed. Check build_log.txt.\""
            ]
        },
        {
            "user": "sys_bob",
            "name": "Investigating a failing CI build",
            "category": "Variant",
            "zone": "/var/log/ci",
            "commands": [
                "tail -n 50 build_log.txt",
                "ssh dev_alice@awesome-app-server:/home/dev_alice/projects/awesome-app",
                "git checkout develop",
                "git pull",
                "npm install",
                "npm test"
            ]
        },
        {
            "user": "dev_alice",
            "name": "Develop new feature",
            "category": "Routine",
            "zone": "/home/alice/projects/webapp",
            "commands": [
                "git checkout -b feature/user-auth",
                "vim src/auth/login.js",
                "npm install bcrypt --save",
                "git add src/auth/login.js package.json",
                "git commit -m 'feat: Implement user login endpoint'",
                "git push origin feature/user-auth"
            ]
        },
        {
            "user": "svc_ci",
            "name": "CI pipeline triggered",
            "category": "Routine",
            "zone": "/var/lib/jenkins/workspace/webapp-pipeline",
            "commands": [
                "git fetch origin",
                "git checkout feature/user-auth",
                "npm ci",
                "npm test",
                "docker build -t mywebapp:feature-user-auth ."
            ]
        },
        {
            "user": "dev_alice",
            "name": "Address CI feedback",
            "category": "Variant",
            "zone": "/home/alice/projects/webapp",
            "commands": [
                "git pull origin feature/user-auth",
                "vim src/auth/login.js",
                "git add src/auth/login.js",
                "git commit -m 'fix: Correct authentication logic based on CI feedback'",
                "git push origin feature/user-auth"
            ]
        },
        {
            "user": "svc_ci",
            "name": "CI re-run after fix",
            "category": "Routine",
            "zone": "/var/lib/jenkins/workspace/webapp-pipeline",
            "commands": [
                "git fetch origin",
                "git checkout feature/user-auth",
                "npm ci",
                "npm test",
                "docker build -t mywebapp:feature-user-auth-v2 ."
            ]
        },
        {
            "user": "sys_bob",
            "name": "Investigate intermittent server error",
            "category": "Anomaly",
            "zone": "/var/log/nginx",
            "commands": [
                "tail -f error.log",
                "grep '502 Bad Gateway' /var/log/nginx/error.log",
                "systemctl status nginx",
                "journalctl -u nginx --since '1 hour ago'"
            ]
        },
        {
            "user": "dev_alice",
            "name": "Merge feature branch",
            "category": "Routine",
            "zone": "/home/alice/projects/webapp",
            "commands": [
                "git checkout main",
                "git pull origin main",
                "git merge feature/user-auth",
                "git push origin main"
            ]
        },
        {
            "user": "sys_bob",
            "name": "Restart failing service",
            "category": "Anomaly",
            "zone": "/etc/systemd/system",
            "commands": [
                "systemctl restart webapp.service",
                "journalctl -u webapp.service -f"
            ]
        }
    ],
    "assets": {
        "vuln_commands": [],
        "honeytoken_commands": []
    },
    "personas": {
        "dev_alice": {
            "work_hours": [
                9,
                17
            ],
            "probability": 0.8,
            "home_dir": "/home/dev_alice",
            "scenes": [
                {
                    "name": "Routine Coding",
                    "category": "Routine",
                    "zone": "/var/www/project",
                    "commands": [
                        "vim main.py",
                        "git status",
                        "git add .",
                        "git commit -m 'Update logic'",
                        "git push origin main"
                    ]
                },
                {
                    "name": "Debug Session",
                    "category": "Variant",
                    "zone": "/var/log",
                    "commands": [
                        "tail -f error.log",
                        "grep 'Error' syslog",
                        "cd /var/www/project",
                        "vim config.py"
                    ]
                },
                {
                    "name": "Emergency Fix",
                    "category": "Anomaly",
                    "zone": "/var/www/project",
                    "commands": [
                        "git checkout -b hotfix-auth",
                        "vim auth.py",
                        "git commit -m 'CRITICAL: Fix auth bypass'",
                        "git push origin hotfix-auth"
                    ]
                }
            ]
        },
        "sys_bob": {
            "work_hours": [
                8,
                16
            ],
            "probability": 0.6,
            "home_dir": "/home/sys_bob",
            "scenes": [
                {
                    "name": "System Check",
                    "category": "Routine",
                    "zone": "/etc",
                    "commands": [
                        "df -h",
                        "top -b -n 1",
                        "systemctl status nginx"
                    ]
                },
                {
                    "name": "Log Rotation Manual",
                    "category": "Variant",
                    "zone": "/var/log",
                    "commands": [
                        "tar -czf logs_backup.tar.gz *.log",
                        "mv logs_backup.tar.gz /backup/"
                    ]
                }
            ]
        },
        "svc_ci": {
            "work_hours": [
                0,
                23
            ],
            "probability": 0.0,
            "home_dir": "/var/lib/jenkins",
            "scenes": [
                {
                    "name": "Build Pipeline",
                    "category": "Routine",
                    "zone": "/var/lib/jenkins/workspace",
                    "commands": [
                        "./mvnw clean install",
                        "docker build -t app:latest .",
                        "docker push registry.local/app:latest"
                    ]
                }
            ]
        }
    }
}